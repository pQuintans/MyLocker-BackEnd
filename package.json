{
  "name": "mylocker-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "if exist dist ( rmdir /s/q dist ) && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "jest",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm-run-migrations": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "clean": "if exist dist ( rmdir /s/q dist ) && mkdir dist",
    "build-babel": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.3",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  }
}
